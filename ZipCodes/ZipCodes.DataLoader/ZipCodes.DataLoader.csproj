  <ItemGroup>
    <ProjectReference Include="..\ZipCodes.ServiceDefaults\ZipCodes.ServiceDefaults.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Update="zip_codes_states-all.csv">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
